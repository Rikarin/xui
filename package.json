{
  "name": "xui",
  "version": "1.41.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "nx serve --port 4800",
    "build": "nx build",
    "build:theme": "sass --no-source-map ./libs/xui/src/themes/dark.theme.scss:./dist/libs/xui/dark.theme.css",
    "test": "nx test",
    "lint": "nx lint",
    "style-lint": "stylelint **/*.scss",
    "e2e": "nx component-test xui",
    "format": "nx format:write --all",
    "format:check": "nx format:check",
    "copy-readme": "cp README.md libs/xui/README.md && git add libs/xui/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xui && npm version $VERSION && cd ../.. && git add libs/xui/package.json",
    "release": "yarn format:check && yarn lint && yarn test && git pull origin master && standard-version -a",
    "postrelease": "git push --follow-tags origin master"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "16.1.1",
    "@angular/cdk": "16.1.1",
    "@angular/common": "16.1.1",
    "@angular/compiler": "16.1.1",
    "@angular/core": "16.1.1",
    "@angular/forms": "16.1.1",
    "@angular/material": "16.1.1",
    "@angular/platform-browser": "16.1.1",
    "@angular/platform-browser-dynamic": "16.1.1",
    "@angular/router": "16.1.1",
    "@mdi/angular-material": "^7.2.96",
    "@ngneat/until-destroy": "^10.0.0-beta.0",
    "@ngx-translate/core": "^15.0.0",
    "@nx/angular": "16.3.2",
    "@stackblitz/sdk": "^1.9.0",
    "@types/luxon": "^3.3.0",
    "echarts": "^5.4.2",
    "highlight.js": "^11.8.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "luxon": "^3.3.0",
    "ng-packagr": "16.1.0",
    "ngx-echarts": "^16.0.0",
    "ngx-google-analytics": "^14.0.1",
    "ngx-highlightjs": "^9.0.0",
    "ngx-image-cropper": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^8.5.0",
    "postcss-url": "^10.1.3",
    "rxjs": "^7.8.1",
    "sass.js": "^0.11.1",
    "tslib": "^2.5.3",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.0",
    "@angular-devkit/core": "16.1.0",
    "@angular-devkit/schematics": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "16.1.1",
    "@angular/language-service": "16.1.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@cypress/webpack-dev-server": "^3.4.1",
    "@nx/cypress": "16.3.2",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/linter": "^16.3.2",
    "@nx/web": "16.3.2",
    "@nx/workspace": "16.3.2",
    "nx-cloud": "latest",
    "@schematics/angular": "16.1.0",
    "@types/jest": "29.5.2",
    "@types/node": "~20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cypress": "12.14.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "prettier": "2.8.8",
    "standard-version": "^9.5.0",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm-run-all copy-*"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
