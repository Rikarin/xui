{
  "name": "xui",
  "version": "development",
  "private": false,
  "repository": "github:rikarin/xui",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:ssr": "nx serve-ssr",
    "build": "nx run-many -t build -c production --exclude=web --all",
    "build:ci": "nx affected -t build -c production",
    "build:theme": "sass --no-source-map ./libs/xui/src/themes/dark.theme.scss:./dist/libs/xui/dark.theme.css",
    "build:ssr": "nx server",
    "build:web": "nx build web",
    "test": "nx run-many -t test --all -i",
    "test:ci": "nx affected -t test -i",
    "lint": "nx run-many -t lint --all --quiet",
    "style-lint": "stylelint **/*.scss",
    "e2e": "nx component-test xui",
    "format": "nx format:write --all",
    "format:check": "nx format:check",
    "clean": "nx reset && rm -rf dist",
    "prepare": "husky install",
    "prerender": "nx prerender"
  },
  "dependencies": {
    "@angular/platform-server": "~16.1.0",
    "@nguniversal/express-engine": "~16.1.0",
    "@xui/theme-core": "link:libs/theme-core/src",
    "@xui/theme-default": "link:libs/theme-default/src",
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.0",
    "@angular-devkit/core": "16.1.0",
    "@angular-devkit/schematics": "^16.1.4",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/animations": "16.1.1",
    "@angular/cdk": "16.1.1",
    "@angular/cli": "~16.1.0",
    "@angular/common": "16.1.1",
    "@angular/compiler": "16.1.1",
    "@angular/compiler-cli": "16.1.1",
    "@angular/core": "16.1.1",
    "@angular/forms": "16.1.1",
    "@angular/language-service": "16.1.1",
    "@angular/material": "16.1.1",
    "@angular/platform-browser": "16.1.1",
    "@angular/platform-browser-dynamic": "16.1.1",
    "@angular/router": "16.1.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@cypress/webpack-dev-server": "^3.4.1",
    "@mdi/angular-material": "^7.2.96",
    "@ngneat/until-destroy": "^10.0.0-beta.0",
    "@nguniversal/builders": "~16.1.0",
    "@ngx-translate/core": "^15.0.0",
    "@nrwl/cli": "^15.9.3",
    "@nx/angular": "16.5.0",
    "@nx/cypress": "16.5.0",
    "@nx/devkit": "^16.5.0",
    "@nx/eslint-plugin": "16.5.0",
    "@nx/jest": "16.5.0",
    "@nx/js": "^16.5.0",
    "@nx/linter": "16.5.0",
    "@nx/plugin": "^16.5.0",
    "@nx/workspace": "16.5.0",
    "@semantic-release/exec": "^6.0.3",
    "@stackblitz/sdk": "^1.9.0",
    "@types/jest": "29.5.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "~20.3.1",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "cypress": "12.16.0",
    "domino": "^2.1.6",
    "echarts": "^5.4.2",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "highlight.js": "^11.9.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "luxon": "^3.3.0",
    "ng-packagr": "16.1.0",
    "ngx-deploy-npm": "^6.0.0",
    "ngx-echarts": "^16.0.0",
    "ngx-google-analytics": "^14.0.1",
    "ngx-highlightjs": "^9.0.0",
    "ngx-image-cropper": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "nx": "^16.5.0",
    "postcss": "^8.4.25",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.5.0",
    "postcss-url": "^10.1.3",
    "prettier": "2.8.8",
    "rxjs": "^7.8.1",
    "sass.js": "^0.11.1",
    "semantic-release": "^21.0.7",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "wait-on": "^7.0.1",
    "yargs": "^17.7.2",
    "zone.js": "0.13.1"
  },
  "release": {
    "branches": [
      {
        "name": "master",
        "channel": "latest"
      },
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "linkCompare": false,
          "linkReferences": false
        }
      ],
      [
        "@semantic-release/exec",
        {
          "publishCmd": "nx run-many -t patch-dist --all --new-version ${nextRelease.version} && nx run-many -t deploy --all --tag ${branch.channel}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
