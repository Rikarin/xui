{
  "name": "xui",
  "version": "1.37.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve --port 4800",
    "build": "nx build",
    "build:theme": "sass --no-source-map ./libs/xui/src/theme.scss ./dist/libs/xui/all.css",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx component-test xui",
    "format": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "copy-readme": "cp README.md libs/xui/README.md && git add libs/xui/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xui && npm version $VERSION && cd ../.. && git add libs/xui/package.json",
    "release": "yarn format:check && yarn lint && yarn test && git pull origin master && standard-version -a",
    "postrelease": "git push --follow-tags origin master"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "15.0.0",
    "@angular/common": "15.0.0",
    "@angular/compiler": "15.0.0",
    "@angular/core": "15.0.0",
    "@angular/forms": "15.0.0",
    "@angular/material": "15.0.0",
    "@angular/platform-browser": "15.0.0",
    "@angular/platform-browser-dynamic": "15.0.0",
    "@angular/router": "15.0.0",
    "@mdi/angular-material": "^7.0.96",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngx-translate/core": "^14.0.0",
    "@nrwl/angular": "15.1.1",
    "@nrwl/eslint-plugin-nx": "15.1.1",
    "@stackblitz/sdk": "^1.8.1",
    "@types/luxon": "^3.1.0",
    "echarts": "^5.4.0",
    "highlight.js": "^11.6.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "luxon": "^3.1.0",
    "ng-packagr": "^15.0.0",
    "ngx-echarts": "^15.0.0",
    "ngx-google-analytics": "^14.0.1",
    "ngx-highlightjs": "^7.0.1",
    "ngx-image-cropper": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^7.8.3",
    "postcss-url": "^10.1.3",
    "rxjs": "^7.5.0",
    "tslib": "^2.4.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.0",
    "@angular-eslint/eslint-plugin": "~14.3.0",
    "@angular-eslint/eslint-plugin-template": "~14.3.0",
    "@angular-eslint/template-parser": "~14.3.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.0",
    "@angular/language-service": "15.0.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@cypress/webpack-dev-server": "^3.1.0",
    "@nrwl/cli": "15.1.1",
    "@nrwl/cypress": "15.1.1",
    "@nrwl/jest": "15.1.1",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.1.1",
    "@types/jest": "29.2.3",
    "@types/node": "~18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^11.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-preset-angular": "12.2.2",
    "prettier": "2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm-run-all copy-*"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
