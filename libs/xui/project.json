{
  "projectType": "library",
  "root": "libs/xui",
  "sourceRoot": "libs/xui/src",
  "prefix": "xui",
  "targets": {
    "build-internal": {
      "executor": "@nx/angular:package",
      "options": {
        "tsConfig": "libs/xui/tsconfig.lib.json",
        "project": "libs/xui/ng-package.json"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ],
      "configurations": {
        "production": {
          "tsConfig": "libs/xui/tsconfig.lib.prod.json"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": [
        {
          "target": "build-internal",
          "projects": "self"
        }
      ],
      "options": {
        "command": "gulp post-build",
        "cwd": "libs/xui",
        "project": "ng-package.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/xui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/xui/**/*.ts",
          "libs/xui/**/*.html"
        ]
      }
    },
    "patch-dist": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "gulp patch-dist --version={args.version}",
        "cwd": "libs/xui"
      }
    }
  },
  "tags": []
}
